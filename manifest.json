{
  "manifest_version": 2,

  "name": "template",
  "description": "template",
  "version": "1.0",
  "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "template"
  },
  "web_accessible_resources": ["assets/*"],
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
